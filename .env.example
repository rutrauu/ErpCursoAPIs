# Configurações de Ambiente - Copie para .env e configure

# Ambiente de execução
NODE_ENV=development

# Porta do servidor
PORT=3000

# Configurações JWT
JWT_SECRET=seu_jwt_secret_super_seguro_aqui_troque_em_producao
JWT_EXPIRES_IN=7d

# Configurações de Log (opcional)
LOG_LEVEL=info

# Configurações de Rate Limiting (opcional)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Configurações de CORS (opcional)
CORS_ORIGIN=*